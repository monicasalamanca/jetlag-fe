{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "NEXT_DEBUG=1 next build && next start",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "audit:rendering": "tsx scripts/audit-rendering.ts",
    "audit:rendering:verbose": "tsx scripts/audit-rendering.ts --verbose"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@next/third-parties": "^15.3.8",
    "@vercel/og": "^0.6.5",
    "marked": "^15.0.11",
    "next": "15.3.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "sass": "^1.84.0",
    "tsx": "^4.19.2",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,scss,md}": "prettier --write",
    "*.{ts,tsx,js,jsx}": "eslint --fix"
  },
  "author": {
    "name": "Monica Salamanca"
  },
  "engines": {
    "node": "22.15.0"
  }
}
